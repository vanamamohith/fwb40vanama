extends layout

block content
  h1= title
  - const usa  = ['state','city','cost_of_living']
  - let results;
  script.
    function place(x){
      alert('State: ' + x)
   } 
  div(class='container-fluid')
    div(class='row')
      each results, index in usa 
        results = usa(index) 
        div(class='col-sm-2' onclick='place('+index+')') #{results}